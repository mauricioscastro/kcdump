{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/mauricioscastro/kcdump/main/config.schema.json",
  "title": "kcdump config",
  "description": "kcdump configuration options schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "loglevel": {
      "description": "how much log information wanted",
      "enum": [
        "info",
        "debug",
        "warn",
        "error",
        "fatal",
        "panic"
      ],
      "default": "error"
    },
    "format": {
      "description": "output format",
      "enum": [
        "yaml",
        "json",
        "json_pretty",
        "json_lines",
        "json_lines_wrapped"
      ],
      "default": "json_lines"
    }
  }
}